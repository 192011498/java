import java.io.*;
import java.util.*;
class main
{
    public static void main(String []args){
        int num,count=0;
        System.out.println("enter the number :");
        Scanner sc=new Scanner(System.in);
        num=sc.nextInt();
        while(num>0){
            if(num%2==0){
                num=num/2;
                count=count+1;
            }
            else{
                num=num-1;
                count=count+1;
            }
        }
        System.out.println(count);
    }
}
