import java.io.*;
import java.util.*;
class fibsum
{  
public static void main(String args[])  
{    
 int n1=0,n2=1,n3,i,count=10;       
 int sum=1;  
 for(i=2;i<count;++i)
 {    
  n3=n1+n2;    
      
  n1=n2;    
  n2=n3;
  sum=sum+n3;    
 }  
 System.out.println("fibonacci sum "+sum);  
  
}}  
